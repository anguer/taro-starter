{
  "name": "taro-starter",
  "version": "0.0.1",
  "private": true,
  "description": "taro starter",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "taro": "taro",
    "postinstall": "weapp-tw patch",
    "lint": "eslint . --fix",
    "prepare": "husky || true",
    "reinstall": "rimraf bun.lockb && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install",
    "release": "semantic-release"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "Anguer",
  "dependencies": {
    "@babel/runtime": "^7",
    "@tarojs/components": "3",
    "@tarojs/helper": "3",
    "@tarojs/plugin-framework-react": "3",
    "@tarojs/plugin-platform-alipay": "3",
    "@tarojs/plugin-platform-h5": "3",
    "@tarojs/plugin-platform-harmony-hybrid": "3",
    "@tarojs/plugin-platform-jd": "3",
    "@tarojs/plugin-platform-qq": "3",
    "@tarojs/plugin-platform-swan": "3",
    "@tarojs/plugin-platform-tt": "3",
    "@tarojs/plugin-platform-weapp": "3",
    "@tarojs/react": "3",
    "@tarojs/runtime": "3",
    "@tarojs/shared": "3",
    "@tarojs/taro": "3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@crazejs/lint": "^0.0.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "3",
    "@tarojs/plugin-doctor": "^0.0.13",
    "@tarojs/taro-loader": "3",
    "@tarojs/webpack5-runner": "3",
    "@types/node": "^18",
    "@types/react": "^18",
    "@types/webpack-env": "^1",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "autoprefixer": "^10",
    "babel-preset-taro": "3",
    "commitlint": "^19",
    "eslint": "^8",
    "eslint-config-taro": "3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "husky": "^9",
    "lint-staged": "^15",
    "postcss": "^8.4.35",
    "postcss-rem-to-responsive-pixel": "^6.0.1",
    "prettier": "^3",
    "react-refresh": "^0.11.0",
    "semantic-release": "^21",
    "stylelint": "^14.4.0",
    "tailwindcss": "^3",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4",
    "typescript": "^5",
    "weapp-tailwindcss": "^3",
    "webpack": "5"
  }
